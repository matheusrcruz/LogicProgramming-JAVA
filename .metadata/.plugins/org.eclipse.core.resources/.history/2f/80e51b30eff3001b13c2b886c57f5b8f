package uri;

import java.util.Scanner;

public class Ex1002 {

	public static void main(String[] args) {
		/*
		 * area = Ï€ . raio2.
		 * */
		
		
		double area, raio ;
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite um valor");
		raio =sc.nextDouble();
		area = Math.pow(raio, 2) * Math.PI;
		System.out.println(area);
	}

}
