package uri;

import java.util.Scanner;

public class Ex1004 {

	public static void main(String[] args) {
		int a, b, soma;
		Scanner teclado = new Scanner(System.in);
		
		int a, b, soma;
		a = teclado.nextInt();
		b = teclado.nextInt();
		soma = a + b;
		
		System.out.println("SOMA = " + soma);
		teclado.close();

	}

}
